proc main() {
	a;b;c;d;

    if (a) {
      b;c;
    }
    
    if (a) {
      b;c;
    } 
    else {
      d;e;
    }

    try { a;b; }
    try { a;b; } else { c;d; }
    
    choice { a;b; }
    choice { a;b; } or { c;d; }
    choice { c;b; } or { c;d; } or { e;f; }

	alap {	
		body();
	}
	
	do { a;b;c; } while ( d )
	
	while ( garbage ) do {
		cleanup();
		test;
	}
}

proc cleanup() {
  alap {
   collect; remove;
  }
  choice {
    a;b;c;
  } 
  or {
    d;e;f;
  }
}

proc body() {
 a*;
 #y;
 z+;
}